    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.6.2",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2",
    "vite": "^5.4.11"
  }
}
